   "source": [
    "units = float(input(\"enter electricity units:\"))\n",
    "if units <= 100:\n",
    "    bill = units * 5\n",
    "elif units <= 200:\n",
    "    bill = (100*5) + (units - 100) * 7\n",
    "else: \n",
    "    bill = (100*5) + (100*7) + (units-200)*10 \n",
    "print(f\" total electricity bill: {bill}\")"
   ]
